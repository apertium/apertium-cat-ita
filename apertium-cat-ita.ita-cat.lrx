<!-- **************** ADV **************** -->

  <!-- magari > potser, fins i tot -->
  <rule weight="0.6">
    <match lemma="magari" tags="adv"><select lemma="potser"/></match>
  </rule>

  <!-- quanto > quant, com -->
  <rule weight="0.6">
    <match lemma="quanto" tags="adv"><select lemma="quant"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="tanto" tags="adj.*"/>
    <match tags="n.*"/>
    <match lemma="quanto" tags="adv"><select lemma="com"/></match>
  </rule>
  <rule weight="1.0">
    <or>
      <match lemma="tanto" tags="preadv"/>
      <match lemma="tanto" tags="adv"/>
    </or>
    <or>
      <match tags="adj.*"/>
    <match tags="adv"/>
    </or>
    <match lemma="quanto" tags="adv"><select lemma="com"/></match>
  </rule>

  <!-- a vicenda > per torns, recíprocament -->
  <rule weight="0.6">
    <match lemma="a vicenda" tags="adv"><select lemma="per torns"/></match>
  </rule>

  <!-- dove > on, a on -->
  <rule weight="0.6">
    <match lemma="dove" tags="adv.itg"><select lemma="on"/></match>
  </rule>

  <!-- subito > de seguida, immediatament -->
  <rule weight="0.6">
    <match lemma="subito" tags="adv"><select lemma="de seguida"/></match>
  </rule>

  <!-- sregolatamente > desordenadament, etc. -->
  <rule weight="0.6">
    <match lemma="sregolatamente" tags="adv"><select lemma="desordenadament"/></match>
  </rule>

<!-- **************** N **************** -->

  <!-- compagnia >> companyia, germandat -->
  <rule weight="0.8">
    <match lemma="compagnia" tags="n.*"><select lemma="companyia"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="compagnia" tags="n.*"><select lemma="germandat"/></match>
    <match lemma="di" tags="pr"/>
    <match tags="det.def.*"/>
    <match lemma="anello" tags="n.*"/>
  </rule>

  <!-- dorio > dòric, dori -->
  <rule weight="0.6">
    <match lemma="dorio" tags="n.*"><select lemma="dori"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="dorio" tags="n.m.sg"><select lemma="dòric"/></match>
  </rule>

  <!-- capoluogo > capital, cap -->
  <rule weight="0.6">
    <match lemma="capoluogo" tags="n.*"><select lemma="capital"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="capoluogo" tags="n.*"><select lemma="cap"/></match>
    <match lemma="de" tags="pr"/>
    <repeat from="0" upto="1">
      <match tags="det.*"/>
    </repeat>
    <or>
      <match lemma="comarca" tags="n.*"/>
      <match lemma="municipio" tags="n.*"/>
    </or>
  </rule>

  <!-- cocco > cocoter, coc -->
  <rule weight="0.6">
    <match lemma="cocco" tags="n.*"><select lemma="cocoter"/></match>
  </rule>

  <!-- termine > terme, termini -->
  <rule weight="0.6">
    <match lemma="termine" tags="n.*"><select lemma="terme"/></match>
  </rule>
  <rule weight="1.0">
    <or>
      <match lemma="lungo" tags="adj.*"/>
      <match lemma="breve" tags="adj.*"/>
      <match lemma="medio" tags="adj.*"/>
    </or>
    <match lemma="termine" tags="n.*"><select lemma="termini"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="termine" tags="n.*"><select lemma="termini"/></match>
    <repeat from="0" upto="1">
      <or><match tags="adv"/><match tags="preadv"/></or>
    </repeat>
    <or>
      <match lemma="lungo" tags="adj.*"/>
      <match lemma="breve" tags="adj.*"/>
      <match lemma="medio" tags="adj.*"/>
    </or>
  </rule>

  <!-- mole > mola, mol -->
  <rule weight="0.6">
    <match lemma="mole" tags="n.*"><select lemma="mola"/></match>
  </rule>
  <rule weight="1.0">
    <or>
      <match tags="num"/>
      <match tags="num.*"/>
    </or>
    <match lemma="mole" tags="n.*"><select lemma="mol"/></match>
  </rule>

  <!-- cingolo > eruga (cadena), cíngol (cordó) -->
  <rule weight="0.6">
    <match lemma="cingolo" tags="n.*"><select lemma="eruga"/></match>
  </rule>

  <!-- getto > doll, jet -->
  <rule weight="0.6">
    <match lemma="getto" tags="n.*"><select lemma="doll"/></match>
  </rule>

  <!-- nazionale > nacional, selecció nacional -->
  <rule weight="0.6">
    <match lemma="nazionale" tags="n.*"><select lemma="nacional"/></match>
  </rule>
  <rule weight="1.0">
    <match tags="det.def.f.*"/>
    <repeat from="0" upto="8">
      <match lemma="proprio" tags="adj.*"/>
    </repeat>
    <match lemma="nazionale" tags="n.*"><select lemma="selecció nacional"/></match>
  </rule>

  <!-- calcio > futbol, calci -->
  <rule weight="0.6">
    <match lemma="calcio" tags="n.*"><select lemma="futbol"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="calcio" tags="n.*"><select lemma="calci"/></match>
    <repeat from="0" upto="8">
      <or>
        <match tags="*"/>
        <match tags=""/>
