        </or>
        <match plemma="1" tags="vblex.pp.m.sg"><select plemma="3"/></match>
      </rule>
    </def-macro>

    <!-- p1 = verb ita, p2 = verb cat amb adj en masc sg, p3 = verb cat amb adj en fem sg, p4 = verb cat amb adj en masc pl, p5 = verb cat amb adj en fem pl -->
    <def-macro n="verb_adjmsg_adjfsg_adjmpl_adjfpl" npar="5">
      <rule weight="0.8">
        <match plemma="1" tags="vblex.*"><select plemma="2"/></match>
      </rule>
      <rule weight="0.9">
        <match plemma="1" tags="vblex.*.pl"><select plemma="4"/></match>
      </rule>
      <rule weight="1.0">
        <match lemma="il" tags="prn.tn.*.f.sg"/>
        <match plemma="1" tags="vblex.*.sg"><select plemma="3"/></match>
      </rule>
      <rule weight="1.0">
        <match lemma="il" tags="prn.tn.*.m.pl"/>
        <match plemma="1" tags="vblex.*.sg"><select plemma="4"/></match>
      </rule>
      <rule weight="1.0">
        <match lemma="il" tags="prn.tn.*.f.pl"/>
        <match plemma="1" tags="vblex.*.sg"><select plemma="5"/></match>
      </rule>
      <rule weight="1.0">
        <match tags="np.ant.*.f.sg"/>
        <repeat from="0" upto="2">
          <or>
            <match tags="np.cog.*"/>
            <match tags="" case="Aa"/>
          </or>
        </repeat>
        <match plemma="1" tags="vblex.*.sg"><select plemma="3"/></match>
      </rule>
      <rule weight="1.0">
        <match tags="n.f.*"/>
        <repeat from="0" upto="2">
          <or>
            <match tags="preadv"/>
            <match tags="adj.*"/>
          </or>
        </repeat>
        <match plemma="1" tags="vblex.*.sg"><select plemma="3"/></match>
      </rule>
      <rule weight="1.0">
        <match tags="n.f.*"/>
        <repeat from="0" upto="2">
          <or>
            <match tags="preadv"/>
            <match tags="adj.*"/>
          </or>
        </repeat>
        <match plemma="1" tags="vblex.*.pl"><select plemma="5"/></match>
      </rule>
      <rule weight="1.4">
        <or>
          <match tags="n.*"/>
          <match tags="adj.*"/>
          <match tags="preadv"/>
          <match tags="adv"/>
        </or>
        <match plemma="1" tags="vblex.pp.f.sg"><select plemma="3"/></match>
      </rule>
      <rule weight="1.4">
        <or>
          <match tags="n.*"/>
          <match tags="adj.*"/>
          <match tags="preadv"/>
          <match tags="adv"/>
        </or>
        <match plemma="1" tags="vblex.pp.m.pl"><select plemma="4"/></match>
      </rule>
      <rule weight="1.4">
