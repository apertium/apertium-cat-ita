  </rule>

  <!-- single > solter, senzill -->
  <rule weight="0.6">
    <match lemma="single" tags="adj.*"><select lemma="solter"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="CD"/>
    <match lemma="single" tags="adj.*"><select lemma="senzill"/></match>
  </rule>

  <!-- vero > veritable, vertader, cert -->
  <rule weight="0.6">
    <match lemma="vero" tags="adj.*"><select lemma="veritable"/></match>
  </rule>
  <rule weight="1.0">
    <match tags="vbser.*"/>
    <repeat from="0" upto="1">
      <or><match tags="adv"/><match tags="preadv"/></or>
    </repeat>
    <match lemma="vero" tags="adj.*"><select lemma="cert"/></match>
  </rule>

  <!-- ulteriore > ulterior, addicional -->
  <rule weight="0.6">
    <match lemma="ulteriore" tags="adj.*"><select lemma="ulterior"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="ulteriore" tags="adj.*"><select lemma="addicional"/></match>
    <match tags="n.*"/>
  </rule>

  <!-- immobile > immòbil, immoble -->
  <rule weight="0.6">
    <match lemma="immobile" tags="adj.*"><select lemma="immòbil"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="bene" tags="n.*"/>
    <match lemma="immobile" tags="adj.*"><select lemma="immoble"/></match>
  </rule>

  <!-- bello > bonic, bell -->
  <rule weight="0.6">
    <or>
      <match lemma="bello" tags="adj.*"><select lemma="bonic"/></match>
      <match lemma="bel" tags="adj.*"><select lemma="bonic"/></match>
    </or>
  </rule>
  <rule weight="1.0">
    <match tags="det.*"/>
    <or>
      <match lemma="bello" tags="adj.*"><select lemma="bell"/></match>
      <match lemma="bel" tags="adj.*"><select lemma="bell"/></match>
    </or>
  </rule>
  <rule weight="1.2">
    <match tags="det.*"/>
    <or>
      <match lemma="bello" tags="adj.*"><select lemma="bonic"/></match>
      <match lemma="bel" tags="adj.*"><select lemma="bonic"/></match>
    </or>
    <match tags="n.*"/>
  </rule>

  <!-- comunale > municipal, comunal -->
  <rule weight="0.6">
    <match lemma="comunale" tags="adj.*"><select lemma="municipal"/></match>
  </rule>

  <!-- potente > potent, poderós -->
  <rule weight="0.6">
    <match lemma="potente" tags="adj.*"><select lemma="potent"/></match>
  </rule>

  <!-- successivo > següent, successiu -->
  <rule weight="0.6">
    <match lemma="successivo" tags="adj.*"><select lemma="següent"/></match>
  </rule>

  <!-- ampio > ample, ampli -->
  <rule weight="0.6">
    <match lemma="ampio" tags="adj.*"><select lemma="ample"/></match>
  </rule>

  <!-- sinistro > esquerre, sinistre -->
  <rule weight="0.6">
    <match lemma="sinistro" tags="adj.*"><select lemma="esquerre"/></match>
  </rule>

  <!-- intero > sencer, enter -->
  <rule weight="0.6">
    <match lemma="intero" tags="adj.*"><select lemma="sencer"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="intero" tags="adj.*"><select lemma="enter"/></match>
    <match tags="n.*"/>
  </rule>

  <!-- mercantile > mercantil, mercant -->
  <rule weight="0.6">
    <match lemma="mercantile" tags="adj.*"><select lemma="mercantil"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="marina" tags="n.*"/>
    <match lemma="mercantile" tags="adj.*"><select lemma="mercant"/></match>
  </rule>

  <!-- democratico > democràtic, demòcrata -->
  <rule weight="0.6">
    <match lemma="democratico" tags="adj.*"><select lemma="democràtic"/></match>
  </rule>

  <!-- rosso > vermell, negre -->
  <rule weight="0.6">
    <match lemma="rosso" tags="adj.*"><select lemma="vermell"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="vino" tags="n.*"/>
    <match lemma="rosso" tags="adj.*"><select lemma="negre"/></match>
  </rule>

  <!-- caro > car, benvolgut -->
  <rule weight="0.6">
    <match lemma="caro" tags="adj.*"><select lemma="car"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="caro" tags="adj.*"><select lemma="benvolgut"/></match>
    <match lemma="amico" tags="n.*"/>
  </rule>

  <!-- alterno > altern, molt -->
  <rule weight="0.6">
    <match lemma="alterno" tags="adj.*"><select lemma="altern"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="alterno" tags="adj.*.pl"><select lemma="molt"/></match>
    <match tags="n.*"/>
