  <rule weight="1.0">
    <or>
      <match lemma="primaverile" tags="adj.*"><select lemma="primaveral"/></match>
      <match lemma="estivo" tags="adj.*"><select lemma="estival"/></match>
      <match lemma="autunnale" tags="adj.*"><select lemma="tardorenc"/></match>
      <match lemma="invernale" tags="adj.*"><select lemma="hivernal"/></match>
    </or>
    <match tags="n.*"/>
  </rule>
  <rule weight="1.0">
    <match lemma="temperatura" tags="n.*"/>
    <or>
      <match lemma="primaverile" tags="adj.*"><select lemma="primaveral"/></match>
      <match lemma="estivo" tags="adj.*"><select lemma="estival"/></match>
      <match lemma="invernale" tags="adj.*"><select lemma="hivernal"/></match>
    </or>
  </rule>

  <!-- poliziesco > policial, policíac, detectivesc -->
  <rule weight="0.6">
    <match lemma="poliziesco" tags="adj.*"><select lemma="policial"/></match>
  </rule>

  <!-- casuale > casual, aleatori -->
  <rule weight="0.6">
    <match lemma="casuale" tags="adj.*"><select lemma="casual"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="numero" tags="n.*"/>
