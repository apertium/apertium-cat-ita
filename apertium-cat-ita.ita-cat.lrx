  <rules>

<!-- **************** DET **************** -->

  <!-- un altro > un altre, altre -->
  <rule weight="0.6">
    <match lemma="un altro" tags="det.*"><select lemma="un altre" tags="det.*"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="un altro" tags="det.ind.m.pl"><select lemma="altre" tags="det.*"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="un altro" tags="det.ind.f.pl"><select lemma="altre" tags="det.*"/></match>
  </rule>

  <!-- alcuno > un altre, cap -->
  <rule weight="0.6">
    <match lemma="alcun" tags="det.*"><select lemma="algun" tags="det.*"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="senza" tags="pr"/>
    <match lemma="alcun" tags="det.*"><select lemma="cap" tags="det.*"/></match>
  </rule>
  <rule weight="1.0" c="non c'è alcuna fretta">
    <match lemma="non" tags="adv"/>
     <repeat from="0" upto="2">
       <or>
         <match tags="prn.pro.*"/>
         <match tags="adv"/>
       </or>
     </repeat>
    <or><match tags="vblex.*"/><match tags="vbmod.*"/><match tags="vbhaver.*"/><match tags="vbser.*"/></or>
    <match lemma="alcun" tags="det.*"><select lemma="cap" tags="det.*"/></match>
  </rule>

<!-- **************** PRN **************** -->

  <!-- un altro > un altre, altre -->
  <rule weight="0.6">
    <match lemma="un altro" tags="prn.*"><select lemma="un altre" tags="prn.*"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="un altro" tags="prn.ind.m.pl"><select lemma="altre" tags="prn.*"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="un altro" tags="prn.ind.f.pl"><select lemma="altre" tags="prn.*"/></match>
  </rule>

  <!-- loro > ell, els, los -->
  <rule weight="0.6">
     <match lemma="lui" tags="prn.tn.p3.mf.*"><select lemma="ell" tags="prn.tn.p3.*"/></match>
  </rule>
  <rule weight="1.0">
     <match tags="vblex.*"/>
     <match lemma="lui" tags="prn.tn.p3.mf.pl"><select lemma="li" tags="prn.pro.p3.mf.pl"/></match>
  </rule>
  <rule weight="1.2">
     <or><match tags="vblex.inf"/><match tags="vblex.ger"/></or>
     <match lemma="lui" tags="prn.tn.p3.mf.pl"><select lemma="li" tags="prn.enc.p3.mf.pl"/></match>
  </rule>

  <!-- gli > li, els (cas especial amb "glielo" que no entenc gaire perquè està definit així a apertium-ita) -->
  <rule weight="0.6">
     <match lemma="gli" tags="prn.pro.p3.mf.sp"><select lemma="li" tags="prn.pro.p3.mf.sg"/></match>
  </rule>

<!-- **************** ADJ **************** -->

  <!-- indoor > en pista coberta, indoor -->
  <rule weight="0.5">
    <match lemma="indoor" tags="adj.*"><select lemma="en pista coberta"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="Indoor" tags="adj.*"><select lemma="indoor"/></match>
  </rule>

  <!-- fantomatico > fantasmagòric, misteriós -->
  <rule weight="0.5">
    <match lemma="fantomatico" tags="adj.*"><select lemma="fantasmagòric"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="fantomatico" tags="adj.*"><select lemma="misteriós"/></match>
