  <!-- primaverile, > de primavera, primaveral -->
  <!-- estivo, > d'estiu, estival -->
  <!-- autunnale, > de tardor, tardorenc -->
  <!-- invernale, > d'hivern, hivernal -->
  <rule weight="0.6">
    <or>
      <match lemma="primaverile" tags="adj.*"><select lemma="de primavera"/></match>
      <match lemma="estivo" tags="adj.*"><select lemma="d'estiu"/></match>
      <match lemma="autunnale" tags="adj.*"><select lemma="de tardor"/></match>
      <match lemma="invernale" tags="adj.*"><select lemma="d'hivern"/></match>
