  <!-- reale > real, reial -->
  <rule weight="0.5">
    <match lemma="reale" tags="adj.*"><select lemma="real"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="Reale" tags="adj.*"><select lemma="reial"/></match>
