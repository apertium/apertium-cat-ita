  </rule>

  <!-- vario > variat, divers -->
  <rule weight="0.5">
    <match lemma="vario" tags="adj.*"><select lemma="variat"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="vario" tags="adj.*"><select lemma="divers"/></match>
